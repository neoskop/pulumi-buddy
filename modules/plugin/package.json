{
    "name": "@neoskop/pulumi-buddy-plugin",
    "description": "A Pulumi integration for Buddy",
    "version": "1.0.0",
    "repository": "https://github.com/neoskop/pulumi-buddy",
    "author": "Mark Wecke <wecke@neoskop.de>",
    "license": "MIT",
    "bin": {
        "pulumi-buddy-plugin": "bin/install-plugin.sh"
    },
    "scripts": {
        "build": "tsc && (cd src/grpc && cp *.d.ts ../../dist/grpc)",
        "watch": "tsc -w",
        "pretest": "npm run build",
        "test": "jest",
        "prepare": "npm run generate",
        "generate": "npm run generate:grpc-code && npm run generate:buddy-actions",
        "generate:grpc-code": "./bin/generate-grpc-code.sh",
        "generate:buddy-actions": "./bin/generate-buddy-actions.ts"
    },
    "dependencies": {
        "@grpc/proto-loader": "0.5.3",
        "@neoskop/pulumi-buddy": "1.0.0",
        "@pulumi/pulumi": "^1.0.0",
        "axios": "0.19.2",
        "google-protobuf": "3.11.4",
        "grpc": "1.24.2",
        "injection-js": "2.3.0",
        "protobufjs": "6.8.8",
        "reflect-metadata": "0.1.13",
        "tslib": "1.11.0",
        "yargs": "15.1.0"
    },
    "devDependencies": {
        "@pulumi/sources": "git+https://github.com/pulumi/pulumi",
        "@types/chalk": "2.2.0",
        "@types/cheerio": "0.22.16",
        "@types/fs-extra": "8.1.0",
        "@types/google-protobuf": "3.7.2",
        "@types/jest": "25.1.3",
        "@types/listr": "0.14.2",
        "@types/prettier": "1.19.0",
        "@types/yargs": "15.0.3",
        "chalk": "3.0.0",
        "cheerio": "1.0.0-rc.3",
        "fs-extra": "8.1.0",
        "jest": "25.1.0",
        "jest-circus": "25.1.0",
        "jest-spec-reporter": "1.0.12",
        "listr": "0.14.3",
        "prettier": "1.19.1",
        "rxjs": "6.5.4",
        "ts-jest": "25.2.1",
        "ts-morph": "7.0.0",
        "ts-node": "8.6.2",
        "typescript": "3.8.2"
    }
}
